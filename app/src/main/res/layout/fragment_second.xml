<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@android:color/black"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondFragment">





    <LinearLayout

        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">




            <TextView
                android:id="@+id/czas"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="NaN"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/szerokosc"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="Latitude:\n"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>


            <TextView
                android:id="@+id/dlugosc"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="Longitude:\n"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">


            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0.5dp"

                android:layout_weight="1"
                android:gravity="center"
                android:text="Moonrise"
                android:textColor="@color/white"
                android:textSize="21sp" />

            <TextView
                android:id="@+id/wsch"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="NaN"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">


            <TextView

                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="Moonset"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>


            <TextView
                android:id="@+id/zach"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="nAN"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">


            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="New Moon"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>


            <TextView
                android:id="@+id/wchoazymut"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="NaN"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">


            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="Full Moon:"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>


            <TextView
                android:id="@+id/zachazymut"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="NaN"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">


            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="Moon Phase"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>


            <TextView
                android:id="@+id/zmierzch"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0.5dp"

                android:layout_weight="1"
                android:gravity="center"
                android:maxLength="10"
                android:text="NaN"
                android:textColor="@color/white"
                android:textSize="21sp" />
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal">


            <TextView

                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize="21sp"
                android:text="Synodic Day:"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_margin="0.5dp"/>


            <TextView
                android:id="@+id/cywil"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0.5dp"

                android:layout_weight="1"
                android:gravity="center"
                android:maxLength="10"
                android:text="NaN"
                android:textColor="@color/white"
                android:textSize="21sp" />
        </LinearLayout>


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>